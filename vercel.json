{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "includeFiles": "drizzle/schema.js"
    },
    "api/generateTimetable.js": {
      "maxDuration": 300
    }
  }
}